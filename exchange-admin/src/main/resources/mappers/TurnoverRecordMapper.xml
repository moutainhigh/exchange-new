<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blockeng.admin.mapper.TurnoverRecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.blockeng.admin.entity.TurnoverRecord">
        <id column="id" property="id"/>
        <result column="market_id" property="marketId"/>
        <result column="trade_price" property="tradePrice"/>
        <result column="trade_num" property="tradeNum"/>
        <result column="created" property="created"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, market_id AS marketId, trade_price AS tradePrice, trade_num AS tradeNum, created
    </sql>

</mapper>
